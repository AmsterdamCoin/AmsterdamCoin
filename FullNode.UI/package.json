    "karma-jasmine-html-reporter": "0.2.2",
    "karma-sourcemap-loader": "0.3.7",
    "less-loader": "4.0.5",
    "minimist": "1.2.0",
    "mkdirp": "0.5.1",
    "npm-run-all": "4.1.2",
    "npx": "9.7.1",
    "node-sass": "4.7.2",
    "popper.js": "1.12.9",
    "postcss-custom-properties": "6.2.0",
    "postcss-loader": "2.0.10",
    "postcss-url": "7.3.0",
